{
  "name": "yuefu",
  "version": "2.0.2",
  "description": "A programable music player built for compatibility and extensibility.",
  "main": "dist/yuefu.umd.js",
  "module": "dist/yuefu.es5.js",
  "types": "dist/types/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lwyj123/yuefu.git"
  },
  "author": "lwyj123",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lwyj123/yuefu/issues"
  },
  "homepage": "https://github.com/lwyj123/yuefu#readme",
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "browserify": "^16.2.2",
    "colors": "^1.3.3",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^0.28.11",
    "eslint": "^5.16.0",
    "express": "^4.16.3",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "git-revision-webpack-plugin": "^3.0.3",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "opn": "^5.3.0",
    "postcss-loader": "^2.1.5",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.4",
    "rollup": "^1.9.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.1.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.20.1",
    "sass-loader": "^7.0.1",
    "shelljs": "^0.8.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.21.0",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^23.1.4",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.8.1",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "eventemitter3": "^3.1.0",
    "pushstate-server": "^3.0.1",
    "smoothscroll": "^0.4.0",
    "yuefu-template-compiler": "^1.1.2"
  }
}
